msg read {
   @type: 0x103;
   fd: u16;
   nbytes: u16;
   zero: u16; 

   reply {
        status: u16;
        nbytes: u16 hex;
        zero: u16;
   }
}

msg write {
   @type: 0x104;
   fd: u16;
   nbytes: u16;
   zero: u16; 

   reply {
        status: u16;
        nbytes: u16 hex;
        zero: u16;
   }
}

msg lseek {
    @type: 0x105;
    fd: u16;
    offset: u16;
    whence: u16;
    zero: u16;

    reply {
        status: u16;
        zero: u16;
        offset: u32;
    }
}